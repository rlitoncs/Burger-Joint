<body>
  <%- include('partials/_chefOrders') %>
  <main class="chef-orders-container">
    <h2 class="chef-orders-title">Customers Table</h2>
    <section class="chef-orders-table">
      <table>
        <tr>
          <th> Customer ID </th>
          <th> First Name </th>
          <th> Last Name </th>
          <th> Phone Number </th>
          <th> Email </th>
        </tr>
        <% for (let row of customers) { %>
        <tr>
          <div class="chef-order-data">
            <td><%= row.id %></td>
            <td><%= row.first_name %></td>
            <td><%= row.last_name %></td>
            <td><%= row.phone_number %></td>
            <td><%= row.email %></td>
          </div>
        </tr>
      <% } %>
      </table>
    </section>
  </main>

</body>
</html>